{
  "name": "gameboy-emulator",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "wasm-build": "wasm-pack build emulator --target web",
    "build": "wasm-pack build emulator --release --target web && run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@unocss/reset": "^0.59.4",
    "floating-vue": "^5.2.2",
    "idb": "^8.0.1",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.2",
    "vue-resizables": "^0.7.1",
    "vue3-virtual-scroll-list": "^0.2.1"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.2.2",
    "@iconify-json/pixelarticons": "^1.2.2",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.11",
    "@unocss/transformer-variant-group": "^0.59.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^24.1.3",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.4.2",
    "sass": "^1.83.1",
    "typescript": "~5.4.5",
    "unocss": "^0.59.4",
    "vite": "^5.4.11",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vite-plugin-wasm": "^3.4.1",
    "vue-tsc": "^2.2.0"
  }
}
